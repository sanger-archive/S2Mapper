{
  "default":{
    "description":"searching for pages",
    "calls":[
      {
        "description":"Create the search for barcoded kit",
        "method":"post",
        "url":"/lims-support/searches",
        "request":{
          "search":{
            "user":"username",
            "description":"search for barcoded kit",
            "model":"kit",
            "criteria":{
              "label":{
                "position":"barcode",
                "type":"ean13-barcode",
                "value":"1234567890"
              },
              "comparison":{
                "expires":{
                  ">=": "_DATE_OF_TEST_"
                },
                "amount":{
                  ">=": 1
                }
              }
            }
          }
        },
        "response":{
          "search":{
            "actions":{
              "read":"http://localhost:9292/supportsearches-result-url",
              "first":"http://localhost:9292/supportsearches-result-url/page=1",
              "last":"http://localhost:9292/supportsearches-result-url/page=-1"
            },
            "uuid":"laboratorysearches-result-url"
          }
        }
      },
      {
        "description":"Get first page",
        "method":"get",
        "url":"/supportsearches-result-url/page=1",
        "request":null,
        "response":{
          "actions":{
            "read":"http://localhost:9292/supportsearches-result-url/page=1",
            "first":"http://localhost:9292/supportsearches-result-url/page=1",
            "last":"http://localhost:9292/supportsearches-result-url/page=-1"
          },
          "size":1,
          "kits":[
            {
              "kit":{
                "actions":{
                  "read":"http://localhost:9292/KitUUID",
                  "create":"http://localhost:9292/KitUUID",
                  "update":"http://localhost:9292/KitUUID",
                  "delete":"http://localhost:9292/KitUUID"
                },
                "uuid":"KitUUID"
              }
            }
          ]
        }
      }
    ]
  }
}